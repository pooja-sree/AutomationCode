package com.rev.pagefactory;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.support.FindBy;
import org.openqa.selenium.support.PageFactory;

public class Logiin1 {
	WebDriver driver;
	
	
	
	@FindBy(xpath = "//input[@name='usernameOrNumber']")
	private WebElement Email;
	
	 public Logiin1(WebDriver driver) {
	        this.driver = driver;
	        PageFactory.initElements(driver, Logiin1.class); 
	    }
	 
	 public  void Validlogincredentials(String username, String password) throws InterruptedException {
		 Thread.sleep(1000);
		 Email.sendKeys(username);
			
}}


